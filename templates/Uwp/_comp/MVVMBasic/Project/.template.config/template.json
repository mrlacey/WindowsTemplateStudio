{
  "$schema": "http://json.schemastore.org/template",
  "author": "Microsoft",
  "classifications": [
    "Universal"
  ],
  "name": "MVVMBasic.Project",
  "identity": "MVVMBasic.Project",
  "shortName": "MVVMBasic.Project",
  "tags": {
    "language": "C#",
    "type": "item",
    "wts.type": "composition",
    "wts.platform" : "Uwp",
    "wts.version": "1.0.0",
    "wts.compositionOrder": "0",
    "wts.compositionFilter": "$frontendframework == MVVMBasic & wts.type == project"
  },
  "sourceName": "wts.ItemName",
  "preferNameDirectory": true,
  "PrimaryOutputs": [
    {
      "path": "Helpers/Observable.cs"
    },
    {
      "path": "Helpers/RelayCommand.cs"
    }
  ],
  "symbols": {
    "wts.rootNamespace": {
      "type": "parameter",
      "replaces": "Param_RootNamespace"
    }
  }
}
